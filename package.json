{
  "name": "@coderbyheart/photos-editor",
  "version": "0.0.0-development",
  "description": "Editor for the data used for my photos page",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/coderbyheart/photos-editor.git"
  },
  "keywords": [
    "photo",
    "editor",
    "markdown"
  ],
  "author": "Markus Tacker <m@coderbyheart.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/coderbyheart/photos-editor/issues"
  },
  "homepage": "https://github.com/coderbyheart/photos-editor#readme",
  "scripts": {
    "server": "./node_modules/.bin/nodemon --optimize_for_size --max_old_space_size=920 --gc_interval=100 server/main.ts",
    "ui": "vite"
  },
  "devDependencies": {
    "@nordicsemiconductor/asset-tracker-cloud-code-style": "12.0.40",
    "@nordicsemiconductor/eslint-config-asset-tracker-cloud-typescript": "15.0.9",
    "@nordicsemiconductor/from-env": "2.0.0",
    "@preact/preset-vite": "2.5.0",
    "@swc/core": "1.3.63",
    "@types/js-yaml": "4.0.5",
    "@types/leaflet": "1.9.3",
    "@types/node": "20.3.1",
    "@typescript-eslint/eslint-plugin": "5.59.11",
    "bootstrap": "5.3.0",
    "chalk": "5.2.0",
    "eslint": "8.42.0",
    "eslint-plugin-unicorn": "47.0.0",
    "husky": "8.0.3",
    "js-yaml": "4.1.0",
    "leaflet": "1.9.4",
    "lint-staged": "13.2.2",
    "nodemon": "2.0.22",
    "preact-router": "4.1.1",
    "prettier": "2.8.8",
    "prettier-plugin-organize-imports": "3.2.2",
    "regenerator-runtime": "0.13.11",
    "tsx": "3.12.7",
    "typescript": "5.1.3"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --ext .js,.ts,.jsx,.tsx"
    ],
    "*.{md,json,yaml,yml,html}": [
      "prettier --write"
    ]
  },
  "prettier": "@nordicsemiconductor/asset-tracker-cloud-code-style/.prettierrc"
}
