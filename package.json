{
  "name": "@coderbyheart/photos-editor",
  "version": "0.0.0-development",
  "description": "Editor for the data used for my photos page",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/coderbyheart/photos-editor.git"
  },
  "keywords": [
    "photo",
    "editor",
    "markdown"
  ],
  "author": "Markus Tacker <m@coderbyheart.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/coderbyheart/photos-editor/issues"
  },
  "homepage": "https://github.com/coderbyheart/photos-editor#readme",
  "scripts": {
    "server": "./node_modules/.bin/nodemon --optimize_for_size --max_old_space_size=920 --gc_interval=100 server/main.ts",
    "ui": "vite"
  },
  "devDependencies": {
    "@nordicsemiconductor/asset-tracker-cloud-code-style": "11.0.83",
    "@nordicsemiconductor/eslint-config-asset-tracker-cloud-typescript": "10.0.23",
    "@nordicsemiconductor/from-env": "1.0.1",
    "@preact/preset-vite": "2.2.0",
    "@swc/core": "1.2.197",
    "@types/js-yaml": "4.0.5",
    "@types/leaflet": "1.7.11",
    "@types/node": "17.0.41",
    "@typescript-eslint/eslint-plugin": "5.27.1",
    "bootstrap": "5.1.3",
    "chalk": "5.0.1",
    "eslint": "8.17.0",
    "eslint-plugin-unicorn": "42.0.0",
    "husky": "8.0.1",
    "js-yaml": "4.1.0",
    "leaflet": "1.8.0",
    "lint-staged": "13.0.1",
    "nodemon": "2.0.16",
    "preact-router": "4.0.1",
    "prettier": "2.6.2",
    "prettier-plugin-organize-imports": "2.3.4",
    "regenerator-runtime": "0.13.9",
    "ts-node": "10.8.1",
    "ts-node-dev": "2.0.0",
    "typescript": "4.7.3"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --ext .js,.ts,.jsx,.tsx"
    ],
    "*.{md,json,yaml,yml,html}": [
      "prettier --write"
    ]
  },
  "prettier": "@nordicsemiconductor/asset-tracker-cloud-code-style/.prettierrc"
}
